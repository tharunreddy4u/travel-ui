
{
    "extends": [
        "plugin:flowtype/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "jest": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "DEBUG": false
    },
    "rules": {
        "semi": 2,
        "prettier/prettier": "error"
    }
}